{
  "name": "radical-aws",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "check": "tsc --noEmit --skipLibCheck",
    "build": "./build.cjs",
    "synth": "./build.cjs && NODE_OPTIONS=--enable-source-maps cdk synth -j",
    "diff": "./build.cjs && export NODE_OPTIONS=--enable-source-maps && cdk synth -j && echo && (dist/diff.js; dist/diff.js --region=us-east-1)",
    "deploy": "./build.cjs && NODE_OPTIONS=--enable-source-maps cdk deploy --debug=true --verbose=5",
    "bootstrap": "./build.cjs && NODE_OPTIONS=--enable-source-maps cdk bootstrap"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/http-proxy": "^1.17.9",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.9",
    "esbuild": "^0.17.14",
    "esbuild-plugin-import-glob": "^0.1.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@aws-cdk/cloudformation-diff": "^2.85.0",
    "@aws-sdk/client-cloudformation": "^3.359.0",
    "@aws-sdk/client-iam": "^3.347.1",
    "@aws-sdk/client-secrets-manager": "^3.347.1",
    "aws-cdk": "^2.85.0",
    "aws-cdk-lib": "^2.80.0",
    "cdk-ec2-key-pair": "^3.3.1",
    "colors": "^1.4.0",
    "constructs": "^10.2.58",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "fast-json-stable-stringify": "^2.1.0",
    "http-proxy": "^1.18.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.1",
    "source-map-support": "^0.5.21",
    "xregexp": "^5.1.1",
    "yargs": "^17.7.2"
  }
}